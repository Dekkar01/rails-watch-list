<div class="container">
  <ul>
    <% @bookmarks.each do |bookmark| %>
      <li>
        <%= link_to bookmark.comment, bookmark_path(bookmark) %>
        <%= link_to "Delete" , bookmark_path(bookmark), data:{turbo_method: :delete, turbo_confirm: "Are u sure?"} %>
        </li>
    <% end %>
  </ul>
  <%= link_to "Create new bookmark", new_bookmark_path %>
</div>
